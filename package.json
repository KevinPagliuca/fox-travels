{
  "name": "fox-travels",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Kevin Pagliuca",
    "email": "kevin.pagliuca@outlook.com"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules && yarn",
    "commit": "git cz && node .husky/push.js",
    "prelint": "prettier --write . --loglevel silent",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --max-warnings=0",
    "postlint": "tsc --project tsconfig.json --noEmit",
    "precommit": "git add . && npm run lint && git add ."
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "framer-motion": "^7.5.1",
    "next": "12.3.1",
    "nookies": "^2.5.2",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-avatar": "^5.0.3",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "styled-components": "^5.3.5",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "12.3.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "nextjs-best-eslint-config": "^1.0.8",
    "typescript": "4.8.3"
  }
}
