{
  "name": "fox-travels-frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Kevin Pagliuca",
    "email": "kevin.pagliuca@outlook.com"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next node_modules && yarn",
    "commit": "git cz && node .husky/push.js",
    "precommit": "git add . && npm run lint && git add .",
    "prelint": "prettier --write . --loglevel silent",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --max-warnings=0 --fix",
    "postlint": "tsc --project tsconfig.json --noEmit",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@hookform/resolvers": "^2.9.9",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "axios": "^1.1.3",
    "framer-motion": "^7.5.1",
    "graphql": "^16.6.0",
    "mapbox-gl": "^2.10.0",
    "next": "12.3.1",
    "nookies": "^2.5.2",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "react-loader-spinner": "^5.3.4",
    "react-map-gl": "^7.0.19",
    "react-toastify": "^9.0.8",
    "styled-components": "^5.3.5",
    "swiper": "^8.4.4",
    "yup": "^0.32.11",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/import-types-preset": "^2.2.4",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-codegen/typescript-react-apollo": "^3.3.4",
    "@next/eslint-plugin-next": "^13.0.5",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  }
}
